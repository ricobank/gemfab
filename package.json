{
  "name": "gemfab",
  "version": "0.0.11",
  "main": "./dist/index.js",
  "scripts": {
    "init": "npm i",
    "build": "npm run build:sol && npm run build:ts",
    "build:ts": "npx tsc -b",
    "build:sol": "hardhat compile",
    "pretest": "npm run build",
    "test": "ts-mocha 'test/**'",
    "fmt": "ts-standard --fix task test index.ts",
    "verify": "certoraRun src/gem.sol:Gem --verify Gem:specs/Gem.spec"
  },
  "devDependencies": {
    "@etherpacks/dpack": "^0.0.24",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/mocha": "^9.0.0",
    "debug": "^4.3.2",
    "ethers": "^5.5.3",
    "hardhat": "^2.10.1",
    "minihat": "^0.0.6",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.4.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "ethers-eip712": "^0.2.0"
  }
}
